import java.util.Scanner;

public class Array_2d {
	static Scanner scan=new Scanner(System.in);


	public static int[][] intarr(int m, int n)
	{
		int a[][]=new int[m][n];
		
		for(int i=0;i<m;i++)
		{
			for(int j=0;j<n;j++)
			{
				a[i][j]=scan.nextInt();
			}
		}
		return a;
	}
	
	
	public static double[][] doublearr(int m,int n)
	{
		double b[][]=new double[m][n];
		
		for(int i=0;i<m;i++)
		{
			for(int j=0;j<n;j++)
			{
				b[i][j]=scan.nextDouble();
			}
		}
		return b;
		
	}
	
	public static boolean[][] booleanarr(int m,int n)
	{
		boolean c[][]=new boolean[m][n];
		
		for(int i=0;i<m;i++)
		{
			for(int j=0;j<n;j++)
			{
				c[i][j]=scan.nextBoolean();
			}
		}
		return c;
		
	}
	
	
	public static void main(String[] args) {

		System.out.println("Enter no of rowss");
		int m=scan.nextInt();
		
		System.out.println("Enter no of column");
		int n=scan.nextInt();
		
		
	}

}
